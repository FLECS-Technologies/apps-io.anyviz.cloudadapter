{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-manifest/refs/heads/3.1.0/manifest.schema.json",
    "_schemaVersion": "3.1.0",
    "app": "io.anyviz.cloudadapter",
    "version": "##VERSION##",
    "revision": "##FLECS_VERSION##",
    "image": "flecs.azurecr.io/io.anyviz.cloudadapter",
    "multiInstance": false,
    "editors": [
        {
            "name": "UI",
            "port": 8888,
            "supportsReverseProxy": true
        }
    ],
    "capabilities": [
        "NET_ADMIN"
    ],
    "devices": [
        "/dev/net/tun"
    ],
    "volumes": [
        "anyviz_config:/etc/anyviz"
    ]
}